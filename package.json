{
  "name": "webpack-mpa-template",
  "version": "0.1.0",
  "description": "使用Webpack搭建多页面应用，工程使用传统页面和单页面混合开发",
  "main": "index.js",
  "scripts": {
    "clear": "node ./build/webpack.clear.js",
    "webpack-dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js  --progress",
    "webpack-build": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.js --progress",
    "mock": "nodemon ./mock/server.js",
    "dev": "concurrently -n Clear-Server,Webpack-Server,Mock-Server \"npm run clear\" \"npm run webpack-dev\" \"npm run mock\"",
    "build": "concurrently -n Clear-Server,Webpack-Server \"npm run clear\" \"npm run webpack-build\"",
    "publish": "rm -rf mock/public/* && cp -r dist/** mock/public",
    "view": "cross-env NODE_ENV=prudction npm run mock"
  },
  "repository": "https://github.com/Peachick/webpack-mpa-template.git",
  "author": "Peachick <wsm_1105@163.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
